<h1>New Veterinarians Page</h1>
<form action="/vets/new" method="POST">
  <fieldset>
    <legend>Vet First Name:</legend>
    <label for="first_name"></label>
  <input type="text" id="first_name" name="first_name">
  <legend>Vet Last Name:</legend>
  <label for="last_name"></label>
<input type="text" id="last_name" name="last_name">
  <br>
  <br>

    <legend>Practice No:</legend>
    <label for="practice_no"></label>

    <input type="number" id="practice_no" name="practice_no" value="practice_no">
    <br>
    <br>
    <input type="submit" value="Submit"/>
  </form>















